          "Cliente"
        ],
        "summary": "Retorna a lista de todos os clientes",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Retorna a lista de Clientes com sucesso."
